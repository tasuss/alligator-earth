{
  "name": "all-american-alligator-delivery-system",
  "version": "0.0.0",
  "private": true,
  "description": "This is the root package. Do not build individual packages here",
  "scripts": {
    "\n========== PLAY >> ==========": "",
    "play": "pnpm --filter=aaads play",
    "\n========== WATCH >> ==========": "",
    "watch:play": "pnpm --filter=aaads watch:play",
    "watch": "pnpm --filter='999.pivot' watch:ts:all",
    "\n========== BUILD ALL >> ==========": "",
    "build": "pnpm --filter=* build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "\n========== DEPENDENCIES >> ==========": "",
    "deps:list": "pnpm -r list",
    "deps:outdated": "pnpm -r outdated"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tasuss/a.a.a.d.s.git"
  },
  "author": "T>A>S>U>S>S",
  "license": "None",
  "bugs": {
    "url": "https://github.com/tasuss/a.a.a.d.s/issues"
  },
  "homepage": "https://github.com/tasuss",
  "workspaces": [
    "002.space/*",
    "998.terminal/*",
    "999.pivot/*"
  ],
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "prettier": "^2.5.1",
    "prettier-config-standard": "^5.0.0",
    "typescript": "^4.6.2"
  }
}
