{
    "name": "game",
    "version": "0.0.0",
    "private": false,
    "gillian_says": "this is gillian. build individual packages in a script IN the package.!! Include typescript as a dev dependency bc it is really version dependant ",
    "bin": {
        "alligator": "./run.cjs"
    },
    "scripts": {
        "test": "ava",
        "play": "pnpm build && node ./run.cjs",
        "\n========== BUILD >> ==========": "",
        "build": "pnpm build:ts",
        "build:ts": "tsc --build 000.game",
        "build:dir": "mkdir -p ./dist",
        "\n========== TEST >> ==========": "",
        "test:ts:version": "pnpm exec tsc -v && tsc -v ",
        "\n========== CLEAN >> ==========": "",
        "clean": "pnpm clean:ts && pnpm clean:build",
        "clean:ts": "tsc --build --clean 000.game",
        "clean:build": "rm -rf dist",
        "\n========== FIX >> ==========": "",
        "fix": "prettier --write ./run.cjs",
        "watch": "tsc -b 000.game -w"
    },
    "main": "main.js",
    "devDependencies": {
        "@types/node": "^18.7.23",
        "ava": "^4.3.3",
        "nodemon": "^3.0.1",
        "typescript": "^4.6.2",
        "fs-extra": "^10.1.0"
    },
    "dependencies": {
        "aedes": "^0.48.1",
        "async-mqtt": "2.2.1",
        "clone-deep": "^4.0.1",
        "commander": "^9.4.0",
        "dot": "^1.1.3",
        "rx-lite": "^4.0.8",
        "string": "^3.3.3",
        "typescript-ioc": "^3.2.2"
    }
}